{
    "suggest.noselect": true,
    "suggest.enablePreview": false,
    "suggest.enablePreselect": false,
    "suggest.maxCompleteItemCount": 12,
    "suggest.autoTrigger": "always",
    "suggest.triggerAfterInsertEnter": false,
    "suggest.floatEnable": false,
    "suggest.detailMaxLength": 100,
    "suggest.snippetIndicator": "►",
    "suggest.minTriggerInputLength": 2,
    "suggest.acceptSuggestionOnCommitCharacter": true,
    "suggest.lowPrioritySourceLimit": 5,
    "suggest.highPrioritySourceLimit": 10,
    "suggest.removeDuplicateItems": true,
    "suggest.localityBonus": true,
    
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.useQuickfixForLocations": true,
    "workspace.ignoredFolders": ["$HOME/.cargo/**", "**/.git", "**/node_modules/**"],
    
    "list.autoResize": false,
    "list.maxHeight": 12,
    
    "hover.autoHide": true,
    "hover.target": "float",
    "signature.target": "float",
    "signature.hideOnTextChange": true,
    
    "java.enabled": true,
    "java.home": "",
    "java.configuration.runtimes": [],
    
    "typescript.enabled": true,
    "javascript.enabled": true,
    "html.enabled": true,
    "css.enabled": true,
    "json.enabled": true,
    "yaml.enabled": true,
    "xml.enabled": true,
    "lua.enabled": true,
    
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    
    "diagnostic.messageTarget": "echo",
    "diagnostic.signOffset": 9999999,
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "ℹ",
    "diagnostic.hintSign": "➤",
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.virtualTextPrefix": " ● ",
    "diagnostic.virtualTextLines": 3,
    "diagnostic.virtualTextLineSeparator": " \\ ",
    "diagnostic.showDeprecated": true,
    "diagnostic.autoRefresh": true,
    "diagnostic.refreshOnInsertMode": false,
    
    "coc.preferences.formatOnSaveFiletypes": [],
    
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "prettier.printWidth": 200,
    "prettier.semi": true,
    "prettier.singleQuote": false,
    "prettier.trailingComma": "es5",
    
    "typescript.format.tabSize": 4,
    "typescript.format.insertSpaceAfterCommaDelimiter": true,
    "typescript.format.insertSpaceAfterSemicolonInForStatements": true,
    "typescript.preferences.includePackageJsonAutoImports": "off",
    
    "javascript.format.tabSize": 4,
    "javascript.format.insertSpaceAfterCommaDelimiter": true,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
    
    "html.format.tabSize": 4,
    "html.format.wrapLineLength": 200,
    "html.format.indentInnerHtml": true,
    
    "css.format.spaceAroundSelectorSeparator": true,
    "css.format.braceStyle": "expand",
    
    "json.format.tabSize": 4,
    "yaml.format.tabWidth": 4,
    
    "java.format.settings.profile": "custom",
    "java.format.settings.url": "",
    "java.format.enabled": true,
    
    "languageserver": {
        "lua": {
            "command": "lua-lsp",
            "filetypes": ["lua"]
        }
    },
    "snippets.ultisnips.pythonPrompt": false
}
